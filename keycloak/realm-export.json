{
  "realm": "automation-realm",
  "enabled": true,
  "registrationAllowed": true,
  "registrationEmailAsUsername": false,
  "rememberMe": true,
  "verifyEmail": true,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "clients": [
    {
      "clientId": "automation-client",
      "enabled": true,
      "publicClient": true,
      "redirectUris": ["http://localhost:3030/*"],
      "webOrigins": ["http://localhost:3030"],
      "protocolMappers": [
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "roles",
            "jsonType.label": "String"
          }
        }
      ]
    }
  ],
  "roles": {
    "realm": [
      {"name": "user", "description": "Regular user"},
      {"name": "admin", "description": "Administrator"}
    ]
  },
  "smtpServer": {
    "replyToDisplayName": "MailDev NoReply",
    "starttls": "false",
    "auth": "true",
    "envelopeFrom": "",
    "ssl": "false",
    "password": "maildev",
    "port": "1025",
    "host": "maildev",
    "replyTo": "",
    "from": "no-reply@automation.com",
    "fromDisplayName": "Automation Realm",
    "user": "maildev"
  }
}